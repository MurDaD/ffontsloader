{
  "name": "ffontsloader",
  "version": "0.1.0",
  "description": "Fonts Loader gives you total control over the fonts loading via @font-face.",
  "main": "./dist/fontsloader.js",
  "types": "./dist/fontsloader.d.ts",
  "files": [
    "demo",
    "dist",
    "src",
    ".babelrc",
    ".gitignore",
    "jest.config.js",
    "tsconfig.json",
    "webpack.config.js"
  ],
  "scripts": {
    "test": "npx jest --coverage",
    "test:jest": "npx jest",
    "lint": "eslint ./src/** --ext .ts",
    "lint:fix": "eslint ./src/**  --ext .ts --fix",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "pre-commit": [
    "lint",
    "build:prod"
  ],
  "keywords": [
    "web",
    "font",
    "fonts",
    "webfonts",
    "fontloader",
    "@font-face",
    "webfontloader"
  ],
  "author": "MurDaD (Max Shakhbazov)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@types/css-font-loading-module": "^0.0.7",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@webpack-cli/generators": "^2.4.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "eslint-config-standard": "^16.0.3",
    "goodparts": "^1.3.0",
    "jest": "^27.4.7",
    "jest-mock": "^27.5.1",
    "mini-css-extract-plugin": "^2.5.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MurDaD/ffontsloader.git"
  },
  "bugs": {
    "url": "https://github.com/MurDaD/ffontsloader/issues"
  },
  "homepage": "https://github.com/MurDaD/ffontsloader#readme"
}
